{
  "name": "@jems/di",
  "version": "0.3.5",
  "description": "An implementation of IoC pattern based on dependency injection that allows you to granulate and decouple your libraries or applications. Wrote using SOLID principles and a variety OOP patterns implementations.",
  "main": "distribution/Index.js",
  "types": "distribution/Index.d.ts",
  "files": [
    "distribution"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "setup": "npm install gulp-cli -g && npm install jsdoc -g && npm install typings -g && npm install",
    "transpile": "gulp transpile",
    "test": "gulp test",
    "generate_documentation": "jsdoc -c ./configurations/jsdoc.json",
    "zip": "gulp zip"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JemsFramework/di.git"
  },
  "keywords": [
    "IoC",
    "DI",
    "inversion-of-control",
    "dependency-injection",
    "granulate",
    "decouple"
  ],
  "author": "Francisco Mercedes <franciscomerdot@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JemsFramework/di/issues"
  },
  "homepage": "https://github.com/JemsFramework/di#readme",
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-ftp": "^1.1.0",
    "gulp-mocha": "^4.2.0",
    "gulp-typescript": "^3.1.6",
    "gulp-util": "^3.0.8",
    "gulp-zip": "^4.0.0",
    "mocha": "^3.2.0",
    "typescript": "^2.2.1",
    "vinyl-ftp": "^0.6.0"
  }
}
